{
  "name": "axios-batch",
  "version": "0.0.6",
  "description": "Axios batch calls, with return object for tracking",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchRican/axios-batch.git"
  },
  "main": "./bin/index.js",
  "preferGlobal": false,
  "scripts": {
    "start-dev": "ts-node ./lib/index.ts",
    "build": "tsc",
    "start": "node ./bin",
    "test": "jest",
    "test:ci": "jest --coverage && cat coverage/lcov.info | coveralls"
  },
  "keywords": [
    "axios",
    "parallel",
    "retry",
    "batch",
    "requests"
  ],
  "author": "Paul van Woensel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^14.14.22",
    "@types/uuid": "^8.3.0",
    "axios-mock": "^1.0.1",
    "axios-mock-adapter": "^1.19.0",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "types": "./bin/index.d.ts"
}
